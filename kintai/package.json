    "@angular/forms": "^7.0.4",
    "@angular/http": "^7.0.4",
    "@angular/material": "^7.0.4",
    "@angular/material-moment-adapter": "^7.0.4",
    "@angular/platform-browser": "^7.0.4",
    "@angular/platform-browser-dynamic": "^7.0.4",
    "@angular/router": "^7.0.4",
    "@ngx-formly/bootstrap": "^4.8.2",
    "@ngx-formly/core": "^4.8.2",
    "@ngx-formly/material": "^4.8.2",
    "@swimlane/ngx-datatable": "^14.0.0",
    "animate.css": "^3.7.0",
    "apollo-angular": "^1.3.0",
    "apollo-angular-link-http": "^1.2.0",
    "apollo-cache-inmemory": "^1.3.10",
    "apollo-client": "^2.4.6",
    "apollo-link": "^1.2.3",
    "apollo-link-context": "^1.0.9",
