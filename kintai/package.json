{
  "name": "kintai",
  "version": "0.0.0",
  "scripts": {
    "test": "karma start ./core/karma.conf.js",
    "start:admin": "ng serve --port 4201 admin",
    "start:user": "ng serve --port 4202 user",
    "start:master": "ng serve",
    "codegen": "apollo schema:download --endpoint http://localhost:4000/graphql && apollo codegen:generate --target=typescript --schema=schema.json --queries=\"projects/shared/src/lib/infra/*/*.graphql\" --outputFlat ./projects/shared/src/lib/infra/types",
    "deploy:admin": "ng build admin -c production && rsync -auvz ./dist/admin/ test-kintai-docker.gkline.co.jp:~/Kintai-System/03_web_server/build/admin/",
    "deploy:user": "ng build user -c production && rsync -auvz ./dist/user/ test-kintai-docker.gkline.co.jp:~/Kintai-System/03_web_server/build/user/",
    "deploy:master": "ng build -c production && rsync -auvz ./dist/master/ test-kintai-docker.gkline.co.jp:~/Kintai-System/03_web_server/build/master/",
    "deploy:backend": "rsync -auvz ./backend/ test-kintai-docker.gkline.co.jp:~/Kintai-System/03_web_server/build/backend/ --exclude 'node_modules' --exclude '.env' --exclude '__tests__'",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.4",
    "@angular/cdk": "^7.0.4",
    "@angular/common": "^7.0.4",
    "@angular/compiler": "^7.0.4",
